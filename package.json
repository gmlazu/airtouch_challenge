{
  "name": "airtouch_challenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "start:dev": "node build/server.js",
    "start:docker": "docker container start airtouch_challenge",
    "copyfiles": "cd src && ../node_modules/copyfiles/copyfiles ./**/*.key ../build && ../node_modules/copyfiles/copyfiles ./**/*.pem ../build",
    "build": "rm -rf ./build && tsc && npm run copyfiles",
    "build:docker": "npm run build && docker-compose build"
  },
  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "bson": "^3.0.2",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.3.0",
    "mongodb": "^3.1.8",
    "mongoose": "^5.6.4",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "typegoose": "^5.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bson": "^1.0.11",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/mongodb": "^3.1.14",
    "@types/mongoose": "^5.3.12",
    "@types/multer": "^1.3.7",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "typescript": "^3.1.5"
  }
}
